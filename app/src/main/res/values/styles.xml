<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="SpUsage">

    <style name="white_bold_14">
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="gray_medium_12">
        <item name="android:fontFamily">@font/raleway_medium</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="gray_medium_16" parent="gray_medium_12">
        <item name="android:textSize">16dp</item>
    </style>

    <style name="gray_medium_14" parent="gray_medium_12">
        <item name="android:textSize">14dp</item>
    </style>

    <style name="gray_bold_16">
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="white_medium_14">
        <item name="android:fontFamily">@font/raleway_medium</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="white_medium_16" parent="white_medium_14">
        <item name="android:textSize">16dp</item>
    </style>

    <style name="info_dialog_button">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/info_dialog_button_label_color</item>
        <item name="android:backgroundTint">@color/info_dialog_button_backg_color</item>
    </style>

    <style name="FullScreenDialogTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="ProgressDialogTheme" parent="FullScreenDialogTheme">
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@color/progress_bg_50</item>
    </style>

</resources>
